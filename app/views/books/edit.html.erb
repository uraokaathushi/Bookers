<h1>Editing Book</h1>
<%= form_with model: @book, url: update_book_path(@book.id), method: :patch do |f| %>

<tr>
 <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_field :body %>

  <h4><%= f.submit 'Update Book'%></h4>
</tr>
<%= link_to "Show", book_path(@book.id) %>|
<%= link_to "Back", index_book_path(@book) %>

<% end %>

